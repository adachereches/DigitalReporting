	' Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
	' 
	' To open Help, press F1.

	Public Sub Main()
        '
        Dts.TaskResult = ScriptResults.Success
          If CInt(Dts.Variables("TotalExceptionCount").Value) > 0 Then
            Dts.Variables("ETLStatus").Value = "Succeeded with Warnings"
            Dts.TaskResult = ScriptResults.Failure
        End If
        If CInt(Dts.Variables("TotalErrorsCount").Value) > 0 Then
            Dts.Variables("ETLStatus").Value = "Succeeded with Errors"
            Dts.TaskResult = ScriptResults.Failure
        End If
        If CInt(Dts.Variables("TotalErrorsCount").Value) = 0 And CInt(Dts.Variables("TotalExceptionCount").Value) = 0 Then
            Dts.Variables("ETLStatus").Value = "Succeeded"
            Dts.TaskResult = ScriptResults.Success
        End If




