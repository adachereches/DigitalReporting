          DTS:Expression="&quot;SELECT&#xA;s.ROW_ID,&#xA;s.NAME,&#xA;xm.ROW_ID AS XM_ROW_ID,&#xA;xm.LAST_UPD AS XM_LAST_UPD,&#xA;xm.NAME AS XM_NAME,&#xA;xm.TYPE AS XM_TYPE,&#xA;xm.ATTRIB_35 AS XM_PROD_CAT6,&#xA;xm.X_PROD_CHANNEL AS XM_PROD_CHANNEL&#xA;FROM&#xA; SIEBEL.S_PROD_INT s&#xA;LEFT OUTER JOIN&#xA;(SELECT&#xA;ROW_ID,&#xA;LAST_UPD,&#xA;PAR_ROW_ID,&#xA;NAME,&#xA;TYPE,&#xA;ATTRIB_35,&#xA;X_PROD_CHANNEL,&#xA;ROW_NUMBER() OVER (PARTITION BY PAR_ROW_ID ORDER BY LAST_UPD DESC) AS UPD_SEQ&#xA;FROM SIEBEL.S_PROD_INT_XM) xm&#xA;ON&#xA;s.ROW_ID = xm.PAR_ROW_ID&#xA;AND 1 = xm.UPD_SEQ &quot;"
          DTS:IncludeInDebugDump="2345"
          DTS:Namespace="User"
          DTS:ObjectName="SiebelQuery">
          <DTS:VariableValue
            DTS:DataType="8">SELECT
s.ROW_ID,
s.NAME,
xm.ROW_ID AS XM_ROW_ID,
xm.LAST_UPD AS XM_LAST_UPD,
xm.NAME AS XM_NAME,
xm.TYPE AS XM_TYPE,
xm.ATTRIB_35 AS XM_PROD_CAT6,
xm.X_PROD_CHANNEL AS XM_PROD_CHANNEL
FROM
 SIEBEL.S_PROD_INT s
LEFT OUTER JOIN
(SELECT
ROW_ID,
LAST_UPD,
PAR_ROW_ID,
NAME,
TYPE,
ATTRIB_35,
X_PROD_CHANNEL,
ROW_NUMBER() OVER (PARTITION BY PAR_ROW_ID ORDER BY LAST_UPD DESC) AS UPD_SEQ
FROM SIEBEL.S_PROD_INT_XM) xm
ON
s.ROW_ID = xm.PAR_ROW_ID
AND 1 = xm.UPD_SEQ </DTS:VariableValue>
