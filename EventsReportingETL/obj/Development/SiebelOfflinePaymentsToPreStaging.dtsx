                Name="ScriptMain.vb"
                Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic 2008.
' The ScriptMain is the entry point class of the script.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum
	

	' The execution engine calls this method when the task executes.
	' To access the object model, use the Dts property. Connections, variables, events,
	' and logging features are available as members of the Dts property as shown in the following examples.
	'
	' To reference a variable, call Dts.Variables("MyCaseSensitiveVariableName").Value
	' To post a log entry, call Dts.Log("This is my log text", 999, Nothing)
	' To fire an event, call Dts.Events.FireInformation(99, "test", "hit the help message", "", 0, True)
	'
	' To use the connections collection use something like the following:
	' ConnectionManager cm = Dts.Connections.Add("OLEDB")
	' cm.ConnectionString = "Data Source=localhost;Initial Catalog=AdventureWorks;Provider=SQLNCLI10;Integrated Security=SSPI;Auto Translate=False;"
	'
	' Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
	' 
	' To open Help, press F1.

	Public Sub Main()
		'
        ' Available count variables
        '
        'Dts.Variables("StepRecordCount").Value = CInt(Dts.Variables("StepRecordCount").Value)
        'Dts.Variables("StepSuccessfulCount").Value = CInt(Dts.Variables("StepSuccessfulCount").Value)
        'Dts.Variables("StepErrorCount").Value = CInt(Dts.Variables("StepErrorCount").Value)

        'Dts.Variables("StepExtractCount").Value = CInt(Dts.Variables("StepExtractCount").Value)
        'Dts.Variables("StepDeleteCount").Value = CInt(Dts.Variables("StepDeleteCount").Value)
        'Dts.Variables("StepInsertCount").Value = CInt(Dts.Variables("StepInsertCount").Value)
        'Dts.Variables("StepUpdateCount").Value = CInt(Dts.Variables("StepUpdateCount").Value)


        Dts.Variables("StepRecordCount").Value = CInt(Dts.Variables("StepExtractCount").Value)
        Dts.Variables("StepSuccessfulCount").Value = CInt(Dts.Variables("StepExtractCount").Value) _
                                                - CInt(Dts.Variables("StepExceptionCount").Value) _
                                                - CInt(Dts.Variables("StepErrorCount").Value)
        If Dts.Variables("StepSuccessfulCount").Value < 0 Then
            Dts.Variables("StepSuccessfulCount").Value = 0
        End If
        Dts.Variables("StepInsertCount").Value = CInt(Dts.Variables("StepExtractCount").Value) _
                                                - CInt(Dts.Variables("StepErrorCount").Value)


        Dts.Variables("TotalErrorsCount").Value += CInt(Dts.Variables("StepErrorCount").Value)

        Dts.Variables("TotalExceptionCount").Value += CInt(Dts.Variables("StepExceptionCount").Value)

        If CInt(Dts.Variables("StepExceptionCount").Value) > 0 Then
            Dts.Variables("ETLStepStatus").Value = "Succeeded with Warnings"
        End If
        If CInt(Dts.Variables("StepErrorCount").Value) > 0 Then
            Dts.Variables("ETLStepStatus").Value = "Succeeded with Errors"
        End If
        If CInt(Dts.Variables("StepErrorCount").Value) = 0 And CInt(Dts.Variables("StepExceptionCount").Value) = 0 Then
            Dts.Variables("ETLStepStatus").Value = "Succeeded"
        End If


        Dts.TaskResult = ScriptResults.Success
    End Sub

End Class]]></ProjectItem>
